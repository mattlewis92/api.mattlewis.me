{
  "rules": {
    "quotes": [2, "single"],
    "linebreak-style": [2, "unix"],
    "semi": [2, "always"],
    "strict": 0,
    "eol-last": 0,
    "func-style": 0,
    "require-yield": 0
  },
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "mwl",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  }
}